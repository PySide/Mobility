<?xml version="1.0"?>
<typesystem package="QtMobility.Feedback">
    <load-typesystem name="typesystem_core.xml" generate="no"/>
    <namespace-type name="QtMobility" generate="no"/>

    <object-type name="QtMobility::QFeedbackActuator">
        <enum-type name="Capability"/>
        <enum-type name="State"/>
    </object-type>

    <object-type name="QtMobility::QFeedbackEffect">
        <enum-type name="Duration"/>
        <enum-type name="State"/>
        <enum-type name="ErrorType"/>
        <enum-type name="ThemeEffect"/>
    </object-type>

    <object-type name="QtMobility::QFeedbackHapticsEffect" />
    <object-type name="QtMobility::QFeedbackFileEffect" />

    <object-type name="QtMobility::QFeedbackInterface">
        <enum-type name="PluginPriority" />
    </object-type>

    <object-type name="QtMobility::QFeedbackHapticsInterface">
        <enum-type name="ActuatorProperty" />
        <enum-type name="EffectProperty" />
        <modify-function signature="instance()" remove="all" />
    </object-type>
    <object-type name="QtMobility::QFeedbackFileInterface">
        <modify-function signature="instance()" remove="all" />
    </object-type>
    <object-type name="QtMobility::QFeedbackThemeInterface">
        <modify-function signature="instance()" remove="all"/>
    </object-type>

</typesystem>
